{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "hello-world",
    "labels": {
      "name": "hello-world",
      "role": "hello-world"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "hello-world",
        "image": "crccheck/hello-world:latest",
        "ports": [
          {
            "name": "hello-world",
            "containerPort": 8000
          }
        ],
      }
    ]
  }
}
